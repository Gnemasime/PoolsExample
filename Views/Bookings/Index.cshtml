@model IEnumerable<PoolsExample.Models.Bookings>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Pools.Pool_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timeclass.DayTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cust_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.No_Visitors)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costs)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pools.Pool_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timeclass.DayTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cust_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.No_Visitors)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costs)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Book_Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Book_Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Book_Id })
        </td>
    </tr>
}

</table>
